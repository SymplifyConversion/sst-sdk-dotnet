@page
@model IndexModel
@{
    ViewData["Title"] = "Test with sending cookie";
}
<h1>@ViewData["Title"]</h1>

@foreach (string projectName in Model.Client.ListProjects())
{
    <div>
        <h2>Project Name</h2>
        <p>@projectName</p>
        <h1>Variation</h1>
        <p>
            @Model.Client.FindVariation(projectName, Model.websiteId, Model)
        </p>
    </div>
}
